keywords = {Autonomous motivation,Autonomy,Autonomy support,Control,Embedded social contexts,Intrinsic motivation,Life goals,Motivation,Self-determination,Social contexts},
number = {April 2018},
pages = {1--40},
title = {{Motivation, Personality, and Development Within Embedded Social Contexts: An Overview of Self-Determination Theory}},
