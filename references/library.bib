doi = {10.1037/a0025747},
file = {:Users/willhardy/Documents/Mendeley Desktop/Gaudreau, Verner-Filion - 2012 - Dispositional Perfectionism and Well-Being A Test of the 2 x 2 Model of Perfectionism in the Sport Doma.pdf:pdf},
