publisher = {Elsevier Ltd},
title = {{Motives for participation in prolonged engagement high-risk sports: An agentic emotion regulation perspective}},
url = {http://dx.doi.org/10.1016/j.psychsport.2010.04.002},
volume = {11},
year = {2010}
