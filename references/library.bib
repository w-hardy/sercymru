journal = {Journal of Personality and Social Psychology},
keywords = {mediation by percieved similarity to model's compe,modeling, learned helplessness {\&}},
number = {8},
pages = {900--908},
title = {{Learned helplessness through modeling: The role of perceived similarity in competence}},
