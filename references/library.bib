keywords = {1990,affective,and consequences of organizational,and normative organizational com-,commitment,conducted meta-analyses,continuance,correlates,decade since mathieu and,it has been a,meta-analysis,mitment,of the antecedents,organizational behavior,turnover,work conditions,zajac},
number = {1},
pages = {20--52},
pmid = {2635132},
title = {{Affective, Continuance, and Normative Commitment to the Organization: A Meta-analysis of Antecedents, Correlates, and Consequences}},
