doi = {10.1037/0022-3514.52.1.81},
file = {:Users/willhardy/Documents/Mendeley Desktop/McCrae, Costa - 1987 - Validation of the five-factor model of personality across instruments and observers.pdf:pdf},
issn = {1939-1315},
journal = {Journal of Personality and Social Psychology},
number = {1},
pages = {81--90},
pmid = {3820081},
title = {{Validation of the five-factor model of personality across instruments and observers.}},
