doi = {10.1207/s15327906mbr3904_4},
file = {:Users/willhardy/Documents/Mendeley Desktop/Lee, Song - 2004 - Evaluation of the Bayesian and Maximum Likelihood Approaches in Analyzing Structural Equation Models with Small Sampl.pdf:pdf},
