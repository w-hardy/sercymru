keywords = {2×2 model of perfectionism,Mutual suppression,Partialling,Perfectionistic concerns,Perfectionistic strivings,Psychological adjustment,Psychological maladjustment,Two-factor theory of perfectionism},
month = {jan},
pages = {379--386},
title = {{The advantages of partialling perfectionistic strivings and perfectionistic concerns: Critical issues and recommendations}},
url = {https://ac.els-cdn.com/S0191886916309473/1-s2.0-S0191886916309473-main.pdf?{\_}tid=b14b6874-d9a4-11e7-9e04-00000aacb35d{\&}acdnat=1512468883{\_}e24f6fe0bc77cc642e47cc6a0c5a6bd7 http://linkinghub.elsevier.com/retrieve/pii/S0191886916309473},
