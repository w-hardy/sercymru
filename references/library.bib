isbn = {1469-0292},
issn = {14690292},
journal = {Psychology of Sport and Exercise},
keywords = {Associative and propositional evaluation model,Dual process models,Implicit association test},
pages = {24--31},
publisher = {Elsevier Ltd},
title = {{The effects of an imagery intervention on implicit and explicit exercise attitudes}},
url = {http://dx.doi.org/10.1016/j.psychsport.2014.11.007},
