file = {:Users/willhardy/Documents/Mendeley Desktop/Dishman et al. - 2005 - Self-management strategies mediate self-efficacy and physical activity.pdf:pdf},
isbn = {0749-3797 (Print)},
issn = {07493797},
journal = {American Journal of Preventive Medicine},
