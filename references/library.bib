journal = {Psychology of Sport and Exercise},
keywords = {Acceptance,Athletes,Machine learning,Mindfulness,Psychometric validity,Reliability},
number = {May},
pages = {101545},
publisher = {Elsevier},
title = {{Using genetic algorithms to abbreviate the Mindfulness Inventory for Sport: A substantive-methodological synthesis}},
url = {https://doi.org/10.1016/j.psychsport.2019.101545},
volume = {45},
year = {2019}
}
