@phdthesis{Glendinning2018,
abstract = {Self-determination theory is a theory of personality and motivation that provides one perspective on the social-cognitive dimensions that underpin human behaviour. According to self-determination theory, there are three needs that are universally fundamental for self-determined motivation and psychological well-being. The hypothesis of universal needs suggests that, when satisfied, autonomy, competence and relatedness are equally beneficial for all people, regardless of individual differences in need strength (c.f., Deci {\&} Ryan, 1985; Ryan {\&} Deci, 2002). However, other theories developed within personality and social psychology tend to view needs as learned and varied (e.g., McClelland, 1985; Murray, 1938). As such, there is some debate as to whether the needs described by self-determination theory are universal requirements, or whether they are learned dispositions that vary for all people (Sheldon {\&} Niemiec, 2006). Sense of coherence (Antonovsky, 1979; 1987) is another perspective on psychological health and well-being whose core dimensions appear to share some similarities with self-determination theory. However, the theories have different traditional foci. Research in sense of coherence theory is traditionally concerned with how sense of coherence ultimately leads to survival in the sense that one has no functional limitation (e.g., low levels of depression). On the other hand, empirical research in self-determination theory has historically focused on how basic need satisfaction facilitates optimal psychological well-being and growth orientated behaviour. Because of the difference in traditional foci, research within the framework of sense of coherence and basic needs satisfaction has taken place independently. As such, the relationship between the two theories and associated well-being is yet to be addressed. Chapter 2 of this thesis provides evidence that the benefits of need satisfaction are in fact not always equal; rather they are dependent on their relative intra-individual importance. Studies 1 to 4 show that the motivation benefits associated with need satisfaction gained via a specific activity depend on intra-individual differences in need importance. Studies 3 and 4 show that for the general population, the effects of need satisfaction on general well-being were equal for all people regardless of the importance attached to each need. Those data support Deci and Ryan's (1985) universal benefits position. However, studies 1 and 4 show that when an individual's sense of identity is highly related to their investment in a specific activity, the association between need satisfaction (via an important activity) and general well-being depends on the intra-individual level of need importance. Those data counter the universal benefits position. Collectively these findings support the position that self-determination theories basic psychological needs are not always universally required for motivation and well-being. Chapter 3 of the present thesis (study 5) provides evidence that there is a considerable conceptual overlap amongst the dimensions of sense of coherence and basic needs perspectives. In addition, satisfaction of basic needs significantly mediated the relationship between sense of coherence and positive well-being (measured via life satisfaction, vitality and informant rated coping effectiveness health/performance), but failed to mediate the relationship between sense of coherence and the absence of psychiatric symptoms. In addition, analyses revealed that sense of coherence was directly associated lower levels of psychiatric symptoms. Collectively these findings are in line with the origin of both theories. That is, sense of coherence appears to have a direct relationship with lower levels of psychiatric illness, whereas basic need satisfaction does not. Instead, basic need satisfaction is associated with positive well-being.},
author = {Glendinning, Freya},
file = {:Users/willhardy/Documents/Mendeley Desktop/Glendinning - 2018 - Self-Determination Theory An Individual Differences Perspective.pdf:pdf},
school = {Bangor University},
title = {{Self-Determination Theory: An Individual Differences Perspective}},
type = {PhD},
year = {2018}
}
@article{Burawoy2003,
abstract = {This paper explores the ethnographic technique of the focused revisit-rare in soci- ology but common in anthropology-when an ethnographer returns to the site of a previous study. Discrepancies between earlier and later accounts can be attributed to differences in: (1) the relation of observer to participant, (2) theory brought to the field by the ethnographer, (3) internal processes within the field site itself, or (4) forces external to the field site. Focused revisits tend to settle on one or another of these four explanations, giving rise to four types of focused revisits. Using examples, the limits of each type of focused revisit are explored with a view to developing a reflexive ethnography that combines all four approaches. The principles of the fo- cused revisit are then extended to rolling, punctuated, heuristic, archeological, and valedictory revisits. In centering attention on ethnography-as-revisit sociologists directly confront the dilemmas of participating in the world they study-a world that undergoes (real) historical change that can only be grasped using a (constructed) theoretical lens.},
author = {Burawoy, Michael},
doi = {10.2307/1519757},
file = {:Users/willhardy/Documents/Mendeley Desktop/Burawoy - 2003 - Revisits An Outline of a Theory of Reflexive Ethnography.pdf:pdf},
isbn = {0003-1224},
issn = {00031224},
journal = {American Sociological Review},
number = {5},
pages = {645},
pmid = {2136792088324102435},
title = {{Revisits: An Outline of a Theory of Reflexive Ethnography}},
volume = {68},
year = {2003}
}
