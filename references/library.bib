journal = {Psihologijske Teme},
keywords = {Five-factor personality traits,Hardiness,Measurement invariance,Negative affect,Physical symptoms,Positive affect},
number = {3},
pages = {487--507},
title = {{The structure of hardiness, its measurement invariance across gender and relationships with personality traits and mental health outcomes}},
