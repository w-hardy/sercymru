journal = {Journal of Expertise},
keywords = {deliberate practice,feature selection,pattern recognition,resilience.,talent development,talent identification},
number = {2},
pages = {92--120},
title = {{The identification of ‘Game Changers' in England cricket's developmental pathway for elite spin bowling: a machine learning approach}},
