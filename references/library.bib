@article{George1998,
abstract = {Self and identity are central concepts in the social and behavioral sciences for multiple reasons. At least three major research traditions focus on the self. First, sociologists view the self as a primary bridge between social structures and individual attitudes and behaviors, as a mechanism by which social structures and individuals affect and are affected by each other. Second, self and identity are viewed as cornerstones of well-being. Thus, social and behavioral scientists have documented important links between the self and physical and mental health, role performance, the quality of interpersonal relationships, and subjective well-being. Third, the self is conceptualized as a central motivating force in human behavior. Issues as diverse as self-selection into specific environments, defense mechanisms, and the desire and ability to break addictive behaviors have been informed by attention to the motivational force of the self. This latter tradition serves as the conceptual bedrock of this paper. This paper examines three components of the self: self-efficacy, self-esteem, and a sense of authenticity. The first two have received substantial theoretical and empirical attention; the latter, much less. I argue that this unequal distribution of scientific inquiry is a result of too much emphasis on the motivation to protect the self, to the neglect of motivation to enhance the self. I make a case for the particular importance of examining self-enhancement in late life. DEFINITIONAL ISSUES At the outset, it is important to clarify several concepts related to self and identity. The ways those concepts are used here are compatible with the bulk of theory and research to date. Nonetheless, scholars sometimes put slightly differing spins on these terms; thus, brief attention to defini-tional issues is warranted. For our purposes, the terms self and identity will be used interchange-ably to refer to the individuals global view of himself or herself. Some schol-ars reserve the term identity for components of the self that are based on experience in social roles (e.g., Burke {\&} Reitzes, 1981). But most view iden-tity as encompassing more than that (Gecas, 1982; Gecas {\&} Burke, 1995; Stryker, 1987). Three specific components of the self will be the primary emphasis of this paper: self-efficacy, self-esteem, and a sense of authentic-ity. In line with previous work, self-efficacy refers to perceptions that one has basic control over one's life (Bandura, 1977; Gecas, 1989; Seligman, 1975). Of particular emphasis here is the corollary notion that self-efficacy is essentially a judgement about self-competence. That is, people who view themselves as self-efficacious believe that they are sufficiently competent to achieve the ends they want and to avoid those they do not want. Also in line with previous theory and research, self-esteem refers to the evaluative part of the self—i.e., to feelings of self-worth (George, 1995; Rosenberg, 1979). Self-esteem is probably the most studied component of the self and is viewed as especially important for mental health. In contrast to self-ef-ficacy and self-esteem, sense of authenticity has received limited attention. For our purposes, and compatible with the views of major figures in this emerging research tradition, sense of authenticity refers to the sense that one's life, both public and private, reflects one's real self (Goffman, 1959; McGuire, 1984; Swann, 1990). All three of these components of identity apply at both general and specific levels. Thus, we can assess individuals' overall or global perceptions of self-efficacy, self-esteem, and sense of authenticity; but they also can be assessed for specific domains of life ex-perience (e.g., in the economic or interpersonal realms). Some scholars view these components as processual. Most commonly, it has been posited that self-esteem is, in part, a function of self-efficacy. It also is possible, however, that the causal direction is the opposite of this: that feelings of self-worth increase perceptions of self-efficacy. To date, evi-dence about these competing hypotheses (or demonstrating that the influ-ences are reciprocal) is lacking. Regardless of the causal or temporal processes involved, these three components of self and identity are ex-pected to be interrelated. The nature of these relationships is discussed further below.},
author = {George, Linda K},
file = {:Users/willhardy/Documents/Mendeley Desktop/George - 1998 - Self and Identity in Later Life Protecting and Enhancing the Self.pdf:pdf},
journal = {Aging},
keywords = {2 correspondence should be,aging,aging and human development,authenticity,d,department of sociology,directed to linda k,duke,durham,for the study of,george,identity,north carolina,of sociology and center,ph,self protection,self-enhancement,university},
number = {3},
pages = {133--152},
title = {{Self and Identity in Later Life: Protecting and Enhancing the Self}},
volume = {3},
year = {1998}
}
