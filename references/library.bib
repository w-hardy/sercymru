journal = {Learning Environments Research},
keywords = {1,asynchronous learning,d istance e ducation,deles,distance education,distance learning,eval-,i ntroduction,psychosocial learning environment,uation},
pages = {289--308},
title = {{Development and validation of an instrument for assessing distance education learning environments in higher education: The distance education learning environments survey (DELES)}},
