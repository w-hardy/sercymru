@article{Savage2017,
abstract = {It has recently been argued that performers benefit from trauma (i.e., memorable challenges) during development. To deepen knowledge in this area, we explored perceived traumas in the development of 20 senior-international performers with a multimethods, temporal-based design. Results showed that perceived traumas were primarily sports based, recognized from onset of investment, associated with immediately negative but ultimately positive impact, and negotiated through skills that were brought to, rather than generated by, these experiences. The findings provide an interesting contrast to messages in other early trauma-focused research and promote focus on the process and mechanisms of responding to and recovering from traumatic experiences. 2016 Copyright {\textcopyright} Association for Applied Sport Psychology},
