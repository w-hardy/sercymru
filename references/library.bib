issn = {01918869},
keywords = {Anxiety,Behavioral Activation System,Behavioral Inhibition System,Emotion regulation,Reinforcement Sensitivity Theory},
pages = {68--73},
title = {{A 5-year longitudinal study of the adolescent reinforcement sensitivity as a risk factor for anxiety symptoms in adulthood: Investigating the indirect effect of cognitive emotion regulation}},
volume = {95},
year = {2016}
}
