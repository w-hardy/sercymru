author = {Jones, Gareth and Milligan, James and Llewellyn, David and Gledhill, Adam and Johnson, Mark I.},
doi = {10.1080/1612197X.2015.1069876},
isbn = {1612-197X},
issn = {1557251X},
journal = {International Journal of Sport and Exercise Psychology},
number = {1},
