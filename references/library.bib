@article{Brickman1972,
abstract = {Although psychologists have long been concerned to distinguish rational from nonrational elements in thought processes, surprisingly little attention has been paid to this issue in the study of selective exposure and reactions to expectancy disconfirmation. The present study explored directly how much information subjects sought and how they revised their expectancies when receiving clues that an outcome, either expected or unexpected, positive or negative, had occurred, Revisions were compared to the rational revisions prescribed by a Bayesian model of probabilistic inference. The subjects were 105 students who volunteered to learn their actual course grades in an experiment. Subjects receiving discrepant outcomes were actually more rational in revising their opinions according to a Bayesian model, and in addition tended to seek more information. Following exposure, however, subjects receiving discrepant outcomes were more tense and uncertain about their outcome. It is suggested that in general discrepant information arouses conflicting adeptive tendencies: its informational value and subjects' curiosity move them to explore and make use of it, while at the same dissonance and suspicion move them to reject it.},
author = {Brickman, Philip},
file = {:Users/willhardy/Documents/Mendeley Desktop/Brickman - 1972 - Rational and Nonrational Elements in Reactions to Disconfirmation of Performance Expectancies1.pdf:pdf},
