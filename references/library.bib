pages = {93--122},
pmid = {18573033},
title = {{The Impact of Perfectionistic Self-Presentation on the Cognitive, Affective, and Physiological Experience of a Clinical Interview}},
url = {http://guilfordjournals.com/doi/abs/10.1521/psyc.2008.71.2.93},
