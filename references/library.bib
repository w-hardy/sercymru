isbn = {0017-811X},
issn = {0017811X},
journal = {Harvard Law Review},
number = {6},
pages = {1641--1679},
pmid = {224880413},
primaryClass = {gr-qc},
title = {{Controlling shareholders and corporate governance: Complicating the comparative taxonomy}},
volume = {119},
year = {2006}
}
