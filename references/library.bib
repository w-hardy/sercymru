doi = {10.1080/10615806.2016.1181754},
file = {:Users/willhardy/Documents/Mendeley Desktop/Sladek et al. - 2016 - Social support coping style predicts women's cortisol in the laboratory and daily life the moderating role of soc.pdf:pdf},
issn = {1477-2205},
journal = {Anxiety, stress, and coping},
keywords = {Social support coping style,attentional bias,college women,cortisol response,diurnal cortisol,social stress},
number = {May},
pages = {1--16},
pmid = {27189781},
publisher = {Taylor {\&} Francis},
title = {{Social support coping style predicts women's cortisol in the laboratory and daily life: the moderating role of social attentional biases.}},
url = {http://www.tandfonline.com/doi/abs/10.1080/10615806.2016.1181754},
volume = {5806},
year = {2016}
}
@inproceedings{Hendrikx2014,
abstract = {This paper will present the results from our first winter season (2013-14) of data collection using a global crowd-sourced approach to collect travel behavior data for people moving in avalanche terrain. To date, most studies of the human dimensions of decision making in avalanche terrain has fo- cused on two areas - post-accident analysis using accident reports/interviews and, the development of tools as decision forcing aids. We present an alternate method for understanding decision-making in ava- lanche terrain. Our project combines real-time GPS tracking via a smartphone application, with internet based surveys of backcountry skiers as a method to describe and quantify travel practices in concert with group decision-making dynamics, and demographic data of participants during daily excursions into win- ter backcountry. Preliminary data analysis shows that individual experience levels and regional avalanche danger ratings all influence the ways in which people travel in avalanche terrain. Our results will provide the first analysis of coupled real-time GPS tracking of people moving in avalanche terrain combined with psychographic and demographic correlates. This research will lead to an improved understanding of real- time decision making in avalanche terrain},
