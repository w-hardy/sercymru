@article{Manly2015,
abstract = {Higher education researchers using survey data often face decisions about$\backslash$r$\backslash$nhandling missing data. Multiple imputation (MI) is considered by many statisticians to be$\backslash$r$\backslash$nthe most appropriate technique for addressing missing data in many circumstances. In$\backslash$r$\backslash$nparticular, it has been shown to be preferable to listwise deletion, which has historically$\backslash$r$\backslash$nbeen a commonly employed method for quantitative research. However, our analysis of a$\backslash$r$\backslash$ndecade of higher education research literature reveals that the field has yet to make substantial$\backslash$r$\backslash$nuse of this technique despite common employment of quantitative analysis, and$\backslash$r$\backslash$nthat in research where MI is used, many recommended MI reporting practices are not being$\backslash$r$\backslash$nfollowed. We conclude that additional information about the technique and recommended$\backslash$r$\backslash$nreporting practices may help improve the quality of the research involving missing data. In$\backslash$r$\backslash$nan attempt to address this issue, we develop a set of reporting recommendations based on a$\backslash$r$\backslash$nsynthesis of the MI methodological literature and offer a discussion of these recommendations$\backslash$r$\backslash$noriented toward applied researchers. The recommended MI reporting practices$\backslash$r$\backslash$ninvolve describing the nature and structure of any missing data, describing the imputation$\backslash$r$\backslash$nmodel and procedures, and describing any notable imputation results.},
author = {Manly, Catherine A. and Wells, Ryan S.},
doi = {10.1007/s11162-014-9344-9},
file = {:Users/willhardy/Documents/Mendeley Desktop/Manly, Wells - 2015 - Reporting the Use of Multiple Imputation for Missing Data in Higher Education Research.pdf:pdf},
isbn = {0361-0365},
issn = {1573188X},
journal = {Research in Higher Education},
keywords = {Higher education,Missing data,Multiple imputation,Survey research},
number = {4},
pages = {397--409},
publisher = {Springer Netherlands},
title = {{Reporting the Use of Multiple Imputation for Missing Data in Higher Education Research}},
url = {http://dx.doi.org/10.1007/s11162-014-9344-9},
volume = {56},
year = {2015}
}
@article{Robins,
abstract = {In this chapter, we provide an overview of measures of self-conscious emotions (embar-rassment, guilt, pride, and shame). The goal of the chapter is to help researchers identify and select measures of self-conscious emotions that meet their diverse needs. Self-conscious emotions are typically assessed through either self-report scales or coding of nonverbal behavior. We first summarize the extant self-report measures and then describe nonverbal coding schemes for each self-conscious emotion previously found to have a recognizable nonverbal expression (i.e., embarrassment, pride, and shame). In this section, we summarize the self-report measures available for shame (and humilia-tion), guilt, embarrassment, and pride. For each emotion, we organize the available mea-sures into three sections: (1) trait or dispositional scales, (2) state or online feeling scales, and (3) state and trait scales of related constructs. Within each of the three sections, the scales are ordered chronologically, by date of publication. Reflecting the field's tendency to focus on clinically relevant emotions, considerably more effort has been devoted to de-veloping self-report measures of shame and guilt than pride or embarrassment. As a re-sult, the vast majority of scales included here assess shame and guilt. For each specific scale, we provide (1) relevant references; (2) a brief description of the scale and the way it was developed; (3) an indication of whether the scale is " fre-quently used, " " occasionally used, " or " rarely used, " based on a citation analysis of the scale name and relevant publications (" frequently used " = cited at least 50 times; " rarely 443},
author = {Robins, Richard W. and Noftle, Erik E. and Tracy, Jessica L.},
file = {:Users/willhardy/Documents/Mendeley Desktop/Robins, Noftle, Tracy - Unknown - Assessing Self-Conscious Emotions A Review of Self-Report and Nonverbal Measures SELF-REPORT MEASURES.pdf:pdf},
title = {{Assessing Self-Conscious Emotions A Review of Self-Report and Nonverbal Measures SELF-REPORT MEASURES OF SHAME, GUILT, EMBARRASSMENT, AND PRIDE}},
url = {http://ubc-emotionlab.ca/wp-content/files{\_}mf/assessmentchapter2007.pdf}
}
